    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.0-beta.14",
    "@storybook/addon-links": "^5.3.0-beta.14",
    "@storybook/addons": "^5.3.0-beta.14",
